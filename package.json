{
  "name": "vscode-yuque",
  "displayName": "vscode-yuque",
  "description": "",
  "publisher": "jasonHzq",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.47.0"
  },
  "repository": "git@gitlab.alibaba-inc.com:zongquan.hzq/vscode-yuque.git",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:yuqueEditor.yuque"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "yuqueEditor.yuque",
        "displayName": "yuque",
        "selector": [
          {
            "filenamePattern": "*.yuque"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./ && rm -rf client/build && cd client && UV_THREADPOOL_SIZE=100 NODE_ENV=development node node_modules/.bin/webpack --config config/webpack.config.prod.js && cd ..",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
